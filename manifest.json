{
	"manifest_version": 3,
	"name": "AddVent",
	"version": "1.0",
	"description": "Allows quick addition of events to Google Calendar from any webpage.",
	"permissions": ["activeTab", "storage"],
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["content.js"],
		"run_at": "document_idle"
	  }
	],
	"icons": {
	  "16": "icon.png",
	  "48": "icon.png",
	  "128": "icon.png"
	}
  }